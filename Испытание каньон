var enemy1 ="Treasure Chest";
hero.attack("Treasure Chest");
hero.moveUp();
hero.moveDown();
hero.moveRight();
hero.attack("Door");
hero.moveUp(2);
hero.moveDown(2);
hero.moveLeft(3);
while(true) {
     hero.moveUp(2);
      hero.moveLeft();
        var enemy = hero.findNearestEnemy();
     hero.attack(enemy);
     hero.moveLeft();
       hero.moveRight(2);
       hero.moveDown();
       hero.moveRight();
       
    hero.moveUp();
    hero.moveRight();
}
    
